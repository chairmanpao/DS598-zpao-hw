{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "zpaoDS598df"
		},
		"GitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub1'"
		},
		"zpaoDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'zpaoDataLake'"
		},
		"GitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "chairmanpao"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org"
		},
		"openweathersecret_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://zpao-openweather.vault.azure.net/"
		},
		"zpaoDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zpaostorage1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "zpaoDS598df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"GitHub1_credential": {
						"value": "[parameters('GitHub1_credential')]"
					},
					"zpaoDataLake_accountKey": {
						"value": "[parameters('zpaoDataLake_accountKey')]"
					},
					"GitHub1_properties_typeProperties_username": {
						"value": "[parameters('GitHub1_properties_typeProperties_username')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"openweathersecret_properties_typeProperties_baseUrl": {
						"value": "[parameters('openweathersecret_properties_typeProperties_baseUrl')]"
					},
					"zpaoDataLake_properties_typeProperties_url": {
						"value": "[parameters('zpaoDataLake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}
{
	"name": "Json1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "HttpServer1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"latitude": {
				"type": "string",
				"defaultValue": "42.3601"
			},
			"longitude": {
				"type": "string",
				"defaultValue": "-71.0589"
			},
			"appid": {
				"type": "string",
				"defaultValue": "@activity('GetAPIKey').output.value"
			},
			"start": {
				"type": "string",
				"defaultValue": "@addDays(utcNow(), mul(-7, item()))"
			},
			"end": {
				"type": "string",
				"defaultValue": "@addDays(addDays(utcNow(), mul(-7, item())), 6)"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": "?lat=@{dataset().lat}&lon=@{dataset().lon}&dt=@{dataset().start}&appid=@{activity('GetAPIKey').output.value}"
			}
		},
		"schema": {}
	}
}
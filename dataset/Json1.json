{
	"name": "Json1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "weather",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"lat": {
				"type": "float",
				"defaultValue": 42.3601
			},
			"lon": {
				"type": "float",
				"defaultValue": -71.0589
			},
			"start": {
				"type": "int"
			},
			"end": {
				"type": "int"
			},
			"appid": {
				"type": "string",
				"defaultValue": "@{activity('GetAPIKey').output.value}"
			},
			"dataType": {
				"type": "string",
				"defaultValue": "hour"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": {
					"value": "data/2.5/history/city?lat={lat}&lon={lon}&type=hour&start={start}&end={end}&appid={API key}",
					"type": "Expression"
				}
			}
		},
		"schema": {}
	}
